<Window x:Class="SkiRent.Windows.Rent.ChangeLevelInfo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SkiRent.Windows.Rent"
        mc:Ignorable="d"
        Title="ChangeLevelInfo" Height="250" Width="400">
    <Grid>
        <Border BorderThickness="5" BorderBrush="Black" CornerRadius="5" Margin="5" Padding="5">
            <StackPanel>
                <TextBlock Text="Уровень" Margin="5"/>
                <TextBox x:Name="tbLevel" Margin="5"/>
               
                <Button x:Name="save" Content="Сохранить" Click="ButtonSaveClick" Margin="5"/>
            </StackPanel>
        </Border>
    </Grid>
</Window>
